services:
  greenflux:
    build:
      context: C:\Users\mzaiter\source\repos\GitHub\Development
      dockerfile: src/GreenFlux.Charging.Service/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: GreenFlux.Charging.Service
      target: base
    container_name: GreenFlux.Charging.Service
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://0.0.0.0:3190
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      GREENFLUX_CONNECTIONSTRING: Server=20.241.129.190,1433; Initial Catalog=Test;
        User ID=sa; Password=P@ssw0rd; TrustServerCertificate=true;
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      REDIS_CONNECTIONSTRING: http://cache:6379
    image: greenflux:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/GreenFlux.Charging.Service.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 3190
      target: 3190
    tty: true
    volumes:
    - C:\Users\mzaiter\source\repos\GitHub\Development\src\GreenFlux.Charging.Service:/app:rw
    - C:\Users\mzaiter\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\mzaiter\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\mzaiter\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\mzaiter\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\mzaiter\source\repos\GitHub\Development:/src:rw
  redis:
    environment:
      ALLOW_EMPTY_PASSWORD: "yes"
    image: redis:6.2-alpine
    ports:
    - published: 6379
      target: 6379
    volumes:
    - cache:/data:rw
version: '3.4'
volumes:
  cache: {}
  driver: {}